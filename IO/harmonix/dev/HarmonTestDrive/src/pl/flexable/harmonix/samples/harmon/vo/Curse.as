package pl.flexable.harmonix.samples.harmon.vo {


  [Bindable]
  /**
   *
   * @author Mateusz
   *
   */
  public class Curse {

    /**
     *
     */
    public var name : String;

    /**
     *
     */
    public var teacher : String;

    /**
     *
     */
    public var description : String;


    /**
     *
     * @param name
     * @param teacher
     * @param description
     *
     */
    public function Curse(name : String, teacher : String, description : String = null) {
      this.name = name;
      this.teacher = teacher;
      this.description = description;
    }

  }
}